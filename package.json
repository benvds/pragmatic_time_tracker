{
    "name": "pragmatic_time_tracker",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:fix": "oxlint ./src --ignore-path ./.gitignore --fix",
        "lint:format": "prettier --log-level warn --write \"./**/*.{js,jsx,ts,tsx,html,css,md,json}\" ",
        "lint": "pnpm lint:format && pnpm lint:fix ",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:cov": "vitest run --coverage",
        "test:ui": "vitest --ui"
    },
    "dependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@happy-dom/jest-environment": "^18.0.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.2",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "clsx": "^2.0.0",
        "happy-dom": "^18.0.1",
        "oxlint": "^1.14.0",
        "prettier": "^3.0.0",
        "typescript": "^5.9.2",
        "vite": "^7.1.4",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    }
}
